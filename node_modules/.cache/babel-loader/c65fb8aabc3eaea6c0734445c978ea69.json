>\n\n<script>\nimport { ref } from 'vue';\n\nexport default {\n  name: 'CountrySelect',\n  props: ['countries'],\n  setup ({ countries }, { emit }) {\n    const selected = ref(0);\n\n    return {\n      selected,\n      onChange () {\n        const country = countries.find((item) => item.ID === selected.value);\n        emit('get-country', country);\n      }\n    };\n  }\n};\n</script>\n"],"sourceRoot":""}]}